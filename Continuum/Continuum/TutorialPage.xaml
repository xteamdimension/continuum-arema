            <!--Elemento Panorama uno-->
            <controls:PanoramaItem Header="movimento">
                <ScrollViewer>
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="140" />
                            <RowDefinition Height="140" />
                            <RowDefinition Height="96" />
