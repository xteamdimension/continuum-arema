    <!--LayoutRoot is the root grid where all page content is placed-->
    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!--TitlePanel contains the name of the application and page title-->
        <StackPanel x:Name="TitlePanel" Grid.Row="0" Margin="12,17,0,28">
        	<TextBlock x:Name="PageTitle" Text="CONTINUUM" Margin="9,-7,0,0" Style="{StaticResource PhoneTextTitle1Style}" FontFamily="Font/QUARTZMS.TTF#Quartz MS" RenderTransformOrigin="0.5,0.5" AllowDrop="False">
        		<TextBlock.RenderTransform>
        			<CompositeTransform TranslateX="14"/>
        		</TextBlock.RenderTransform>
        	</TextBlock>
        	<Path Data="M0,0" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="0" Margin="0,0,145,0" Stroke="Black" UseLayoutRounding="False" Width="0"/>
        	<Path Data="M0,0" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="0" Margin="0,0,187,0" Stroke="Black" UseLayoutRounding="False" Width="0"/>
        	<Path Data="M0,0" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="0" Margin="0,0,114,0" Stroke="Black" UseLayoutRounding="False" Width="0"/>
        	<Path Data="M0,0" Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="0" Margin="155,0,0,0" Stroke="Black" UseLayoutRounding="False" Width="0"/>
        	<Path Data="M0,0" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="0" Margin="0,0,227.5,0" Stroke="Black" UseLayoutRounding="False" Width="0"/>
        </StackPanel>

        <!--ContentPanel - place additional content here-->
        <Grid x:Name="ContentPanel" Grid.Row="1" Margin="12,0,12,0">
            
            <!--Create a single button to navigate to the second page which is rendered with the XNA Framework-->
            <Button x:Name="gamePage" Height="100" Content="Start" Click="gamePage_Click" RenderTransformOrigin="0.5,0.5" Margin="-2,288,2,287">
            	<Button.RenderTransform>
            		<CompositeTransform/>
            	</Button.RenderTransform>
            </Button>
            <Button x:Name="tutorial" Height="100" Content="Instructions" RenderTransformOrigin="0.5,0.5" Margin="0,383,0,192" Click="tutorial_Click">
                <Button.RenderTransform>
                    <CompositeTransform/>
                </Button.RenderTransform>
            </Button>
            <Button x:Name="scores" Height="100" Content="Scores" RenderTransformOrigin="0.5,0.5" Margin="2,480,-2,95" Click="scores_Click">
                <Button.RenderTransform>
                    <CompositeTransform/>
                </Button.RenderTransform>
            </Button>
            <Button Content="Credits" Height="100" Margin="2,573,-2,2" Name="credits" RenderTransformOrigin="0.5,0.5" Click="button1_Click">
                <Button.RenderTransform>
                    <CompositeTransform />
                </Button.RenderTransform>
            </Button>
            <!--
            <Button Content="Tutorial" Height="96" HorizontalAlignment="Left" Margin="0,378,0,0" Name="button_tutorial" VerticalAlignment="Top" Width="456" Click="button1_Click" />
            <Button Content="Scores" Height="97" HorizontalAlignment="Left" Margin="0,466,0,0" Name="button_scores" VerticalAlignment="Top" Width="456" />
            -->
            </Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Continuum;component/continuum.jpg" Stretch="Fill" />
        </Grid.Background>
    </Grid>
 
